{
	"name": "franciscpd/skeleton-laravel",
	"description": "Skeleton base site for Laravel 4",
	"keywords": ["framework", "laravel", "Starter Site", "Skeleton"],
	"repositories": [
    {
	    "type": "package",
	    "package": {
	        "name": "cssmin/cssmin",
	        "version": "3.0.1",
	        "dist": { "url": "http://cssmin.googlecode.com/files/cssmin-v3.0.1.php", "type": "file" },
	        "autoload": { "classmap": [ "cssmin-v3.0.1.php" ] }
	    	}
		}
	],
	"require": {
		"laravel/framework": "4.1.*",
		"cartalyst/sentry": "2.0.*",
		"jasonlewis/basset": "4.0.*@dev",
		"cssmin/cssmin": "3.0.*"
	},
	"require-dev": {
		"leafo/lessphp": "v0.3.9"
	},
	"autoload": {
		"classmap": [
			"app/commands",
			"app/controllers",
			"app/models",
			"app/database/migrations",
			"app/database/seeds",
			"app/tests/TestCase.php",
			"app/modules"
		]
	},
	"scripts": {
		"post-install-cmd": [
			"php artisan optimize"
		],
		"pre-update-cmd": [
			"php artisan clear-compiled"
		],
		"post-update-cmd": [
			"php artisan optimize"
		],
		"post-create-project-cmd": [
			"php artisan key:generate"
		]
	},
	"config": {
		"preferred-install": "dist"
	},
	"minimum-stability": "dev"
}
